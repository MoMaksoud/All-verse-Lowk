{
  "expo": {
    "name": "AllVerse GPT",
    "slug": "allverse-gpt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "allversegpt",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1b2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.allversegpt.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "AllVerse GPT needs access to your camera to take photos of items you want to sell.",
        "NSPhotoLibraryUsageDescription": "AllVerse GPT needs access to your photo library to upload product photos.",
        "NSPhotoLibraryAddUsageDescription": "AllVerse GPT needs permission to save photos to your library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.allversegpt.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AllVerse GPT to access your camera to take photos of items."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AllVerse GPT to access your photos to upload product images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eab64647-ba64-4620-8e13-1608845fbdc2"
      }
    },
    "owner": "allversegpt"
  }
}
